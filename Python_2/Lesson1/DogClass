class Dog:
    def __init__(self, name, hair, city, age):
        self.name = name
        self.hair = hair
        self.city = city
        self.age = age
        # self.name = name
        # print("bark", self)
    def get_name(self):
        return self.name
    def get_age(self):
        return self.age
    def set_age(self, age):
        self.age=age

dog = Dog("Bruno", "long hair", "Skopje", 34)
dog.set_age(23)
print(dog.get_name())
print(dog.get_age())
dog_name = "Tim"
dog_age = "12"