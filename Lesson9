def koren(broj):
    square_root = broj**0.5
    print(square_root)

koren(36)


br = 625
k = int(math.sqrt(br))
print(k)


# 1,1,2,3,5 ..

def fibonacci():
    fibonacci_lista = [0, 1]
    for value in range(0, 100):
        sleden_broj = fibonacci_lista[-2] + fibonacci_lista[-1]
        fibonacci_lista.append(sleden_broj)
    return fibonacci_lista



print(fibonacci())








